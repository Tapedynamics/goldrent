{
  "version": 2,
  "buildCommand": "cd goldrent-nextjs && npm install && npm run build",
  "outputDirectory": "goldrent-nextjs/.next",
  "installCommand": "cd goldrent-nextjs && npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/goldrent-nextjs/$1"
    }
  ]
}
